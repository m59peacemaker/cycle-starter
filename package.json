{
  "name": "",
  "version": "",
  "description": "",
  "keywords": [],
  "private": true,
  "author": {
    "name": "",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "license": "VOL",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run watch",
    "build-dir": "mkdir -p www",
    "html": "cp src/index.html www/",
    "watch:html": "npm run html && onchange 'src/index.html' -- npm run html",
    "watch:js": "watchify src/app.js -o www/app.js -p errorify -p [browserify-hmr]",
    "build:js": "browserify src/app.js | uglifyjs -cm > www/app.js",
    "dev-server": "m59-reload-server -p 8080 -d www",
    "setup-reload": "onchange 'www/**/!(*.js)' -- curl -X POST localhost:8080/reload?path={{changed}} 2> /dev/null",
    "watch": "npm-run-all -s build-dir -p watch:html watch:js dev-server setup-reload",
    "build": "npm-run-all -s build-dir -p html build:js",
    "test": "node test"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "dependencies": {
    "@cycle/core": "^6.0.3",
    "@cycle/dom": "^9.1.0",
    "@cycle/http": "^8.2.0",
    "ramda": "^0.19.1",
    "rx": "^4.0.8"
  },
  "devDependencies": {
    "@m59/reload-server": "^1.0.4",
    "babel": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^11.1.0",
    "browserify-hmr": "^0.3.1",
    "errorify": "^0.3.1",
    "npm-run-all": "^1.5.1",
    "onchange": "^2.0.0",
    "watchify": "3.4.0"
  }
}
